{
  "manifest_version": 3,
  "name": "GithubCN",
  "short_name": "GithubCN",
  "description": "Github 汉化插件",
  "version": "1.1",
  "homepage_url": "https://github.com/JQiue/githubCN",
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "js/content.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
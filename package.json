    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "compressing": "^1.10.0",
    "dotenv": "^16.4.1",
    "eslint": "^8.21.0",
    "eslint-plugin-vue": "^9.3.0",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",

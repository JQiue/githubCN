{
  "name": "github_cn",
  "version": "1.8.0",
  "description": "Github 汉化插件",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/JQiue/githubCN"
  },
  "scripts": {
    "publish": "node publish.js",
    "dev": "nodemon",
